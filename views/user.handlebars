<link rel="stylesheet" type="text/css" href="\styles\contact.css">
<!-- THE NAV BAR -->
<div class="topnav text-center">
  <a href="/home">HOME</a>
  <a href="/schedule" class="col-xs-12">SCHEDULE</a>
  <a class="active col-xs-12" href="/user" class="col-xs-12">CONTACTS</a>
  <a href="/forum" class="col-xs-12">FORUM</a>
  <a href="/logout" class="col-xs-12">logout</a>
</div>

<!-- START OF SLIDESHOW  -->
<div id="title-card" class="card bg-dark text-white mt-4">
  <div id="carouselExampleControls" class="carousel slide" data-ride="carousel" data-interval="7000">
    <div class="carousel-inner">
      <div class="carousel-item active">
        <img class="d-block w-100" src="images/240_9x2.5_sample_banner_flyer.jpg">
      </div>
      <div class="carousel-item">
        <img class="d-block w-100" src="images/240_9x2.5_lacrosse_sky.jpg" alt="Second slide">
      </div>
    </div>
    <a class="carousel-control-prev" href="#carouselExampleControls" role="button" data-slide="prev">
      <span class="carousel-control-prev-icon" aria-hidden="true"></span>
      <span class="sr-only">Previous</span>
    </a>
    <a class="carousel-control-next" href="#carouselExampleControls" role="button" data-slide="next">
      <span class="carousel-control-next-icon" aria-hidden="true"></span>
      <span class="sr-only">Next</span>
    </a>
  </div>
  <div class="card-img-overlay">
  </div>
</div>

<div id="master-container" class="container usersContainter">
  <div class="row">
    <div class="col">
      <h3 class="text-center">CONTACTS</h3>
    </div>
  </div>
  <div class="row">
    <div class="col-12 table-spot">
      <table class="table table-striped">
        <thead class="table-head">
          <th scope="col">First Name</th>
          <th scope="col">Last Name</th>
          <th scope="col">Email</th>
          <th scope="col">Phone Number</th>
          <th scope="col">Role</th>
        </thead>
        <tbody>
          {{#each users}}
          <tr class="person">
            <td>{{firstName}}</td>
            <td>{{lastName}}</td>
            <td> {{email}}</td>
            <td>{{phoneNumber}}</td>
            <td>{{role}}</td>
          </tr>
          {{/each}}
        </tbody>
      </table>
    </div>
  </div>

</div>
<footer class="footer">
  <div class="footer-container col-12">
    <div class="row copyright">
      <div class="col">
        <p>Copyright, Team X, 2018 Â©</p>
      </div>
    </div>
    <div class="row">
      <div class="col">
        <div class="icons">
          <i class="fab fa-facebook-square"></i>
          <i class="fab fa-twitter-square"></i>
          <i class="fab fa-instagram"></i>
          <i class="fab fa-youtube-square"></i>
        </div>
      </div>
    </div>
</footer>